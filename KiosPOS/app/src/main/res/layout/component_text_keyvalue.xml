<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/texvw_key_20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:textSize="@dimen/size_12sp"
        android:layout_margin="5dp"
        android:gravity="left"
        android:textColor="@color/colorBlue_06"
        android:text="ORDER DETAILS" />

    <TextView
        android:id="@+id/texvw_value_21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:textSize="@dimen/size_12sp"
        android:layout_margin="5dp"
        android:textStyle="bold"
        android:gravity="left"
        android:textColor="@color/colorBlueBlack_01"
        android:text="SOUNDREANALINE 2017 - \nONE DAY PASS: DAY 2" />

    <RelativeLayout
        android:layout_below="@+id/texvw_value_21"
        android:background="@color/colorBlue_06_t50"
        android:layout_marginTop="@dimen/size_5dp"
        android:layout_width="match_parent"
        android:layout_height="1dp">

    </RelativeLayout>
</RelativeLayout>
